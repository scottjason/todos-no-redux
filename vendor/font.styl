icomoon-font-path ?= "../vendor/fonts"
icon-pencil = "\e905"
icon-pencil2 = "\e906"
icon-file-empty = "\e924"
icon-history = "\e94d"
icon-alarm = "\e950"
icon-floppy-disk = "\e962"
icon-database = "\e964"
icon-undo = "\e965"
icon-bin = "\e9ac"
icon-bin2 = "\e9ad"
icon-plus = "\ea0a"
icon-cross = "\ea0f"
icon-checkmark = "\ea10"
icon-checkmark2 = "\ea11"

@font-face
  font-family 'icomoon'
  src url(icomoon-font-path + '/icomoon.eot?u5vr2c')
  src url(icomoon-font-path + '/icomoon.eot?u5vr2c#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icomoon.ttf?u5vr2c') format('truetype'), url(icomoon-font-path + '/icomoon.woff?u5vr2c') format('woff'), url(icomoon-font-path + '/icomoon.svg?u5vr2c#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-pencil
  &:before
    content icon-pencil
.icon-pencil2
  &:before
    content icon-pencil2
.icon-file-empty
  &:before
    content icon-file-empty
.icon-history
  &:before
    content icon-history
.icon-alarm
  &:before
    content icon-alarm
.icon-floppy-disk
  &:before
    content icon-floppy-disk
.icon-database
  &:before
    content icon-database
.icon-undo
  &:before
    content icon-undo
.icon-bin
  &:before
    content icon-bin
.icon-bin2
  &:before
    content icon-bin2
.icon-plus
  &:before
    content icon-plus
.icon-cross
  &:before
    content icon-cross
.icon-checkmark
  &:before
    content icon-checkmark
.icon-checkmark2
  &:before
    content icon-checkmark2

